    "from tkinter import*\n",
    "root = Tk()\n",
    "def action():\n",
    "    l2 = Label(root,text = \"Abdullah\")\n",
    "    l2.pack()\n",
    "\n",
    "l1 = Label(root,text = 'Hello ubaid!')\n",
    "b1 = Button(root,text = 'click here to know your surname',command = action )\n",
    "b1.pack()\n",
    "l1.pack()\n",
    "\n",
    "root.geometry('200x200')\n",
    "root.mainloop()"
   ]
